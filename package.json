{
  "name": "boat-ticket-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "node server/index.js",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "check-syntax": "node check-syntax.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:dispatcher": "vitest run tests/dispatcher --no-file-parallelism --reporter=verbose",
    "test:seller": "vitest run tests/seller --no-file-parallelism --reporter=verbose",
    "test:owner": "vitest run tests/owner --no-file-parallelism --reporter=verbose",
    "test:all": "npm run test:owner && npm run test:seller && npm run test:dispatcher",
    "e2e": "playwright test --project=chrome",
    "e2e:ui": "playwright test --project=chrome --ui",
    "e2e:headed": "playwright test --project=chrome --headed",
    "diag": "node diag-simple.mjs"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.19",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
